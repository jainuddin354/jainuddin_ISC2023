package nanana;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner jy = new Scanner(System.in);
        int jml = 5;
        String []a =new String [jml];
        for (int b = 0; b < jml; b++){
        System.out.print("index "+b+":");
            a[b]=jy.nextLine();
        }System.out.println("--------out--put--------");
        for(int i = 0; i < jml;i++){
            System.out.print(a[i] +",");
        }
    }
}

    
    

