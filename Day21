package aril;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    System.out.print("Masukkan batasan angka : ");
    Scanner masukan = new Scanner(System.in);
    int a = masukan.nextInt();
    for(int i = 0; i <= a;i++){
        int jumlah = i % 2;
        if(jumlah==1){
            System.out.print(i+ " ");
            System.out.println();
    
    
}
        }    
         
    }
    
}
