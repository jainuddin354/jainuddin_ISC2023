package nanana;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner jy = new Scanner(System.in);
        int a[] = {1,2,3,4,5,6,0,8,9};
        int jml = a.length;
        int min = a[0];
        for(int i = 0;i < jml;i++){
            System.out.print(a[i]+" ");
            if (min > a[i]){
                min = a[i];
            }
        }System.out.println(" ");
     System.out.print("Angak paling kecil :"+min);   
    }
}
