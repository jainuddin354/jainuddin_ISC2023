package day4;

public class Main {
    
    public static void main(String[] args) {
       //deklarasi fariabel
        String nama;
        char huruf_awal;
        int umur;
        float berat;
        double tinggi;
            boolean tidak_tampan;
        
        // next.
        nama = "jainuddin";
        huruf_awal = 'J';
        umur = 20;
        berat = 15.7f;
        tinggi = 150;
        tidak_tampan = false;
        
        //Menampilkan hasil
        System.out.println("Nama Saya \n\n" + nama );
        System.out.println("huruf awalnya yaitu\t\t : " + huruf_awal);
        System.out.println("Umur Saya \t\t : " + umur);
        System.out.println("Berat badan Saya \t\t : " + berat);
        System.out.println("Tinggi Saya \t\t :" + tinggi);
        System.out.println("sya orang tampan\t\t:" + tidak_tampan);
        
        
        
        
    }
    
}
