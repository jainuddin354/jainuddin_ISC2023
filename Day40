package ky;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner jy = new Scanner(System.in);
        System.out.print("masukkan jumlah index : ");
        int a = jy.nextInt();
        int[] b =new int [a];
        for ( int c= 0;c < a ;c++ ){
            System.out.print(" index : ");
            b[c] = jy.nextInt();
        
        }
    }
}
