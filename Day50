package hahahah;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner jay = new Scanner(System.in);
        System.out.print("gaji pokok :");
        int a = jay.nextInt();
        System.out.print("lama lembur :");
        int b =jay.nextInt();
        b*= 55000;
        System.out.println("gaji bersih : "+(a+b));
    }
}
