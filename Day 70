package jay;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner  jy= new Scanner(System.in);
        int jml = 5;
        int []a =new int [jml];
        for (int b = 0; b < jml; b++){
        System.out.print("index "+b+":");
            a[b]=jy.nextInt();
}System.out.println("___________________");
        for(int i = 0; i < jml ;i++){
            System.out.print(a[i] +" ");
        }
        
    }
    
}
