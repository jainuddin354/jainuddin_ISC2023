package aril;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("menti terganteng anda \t ");
        System.out.println("1.gedry^_^");
        System.out.println("2.sya(jainuddin)*-*");
        System.out.println("3.aril -_-");
        System.out.println("4.kholil '_'");
        System.out.print("pilih salah satunya :");
        int jl=input.nextInt();
        switch(jl){
            case 1:
            System.out.println("maaf anda keliru");
            break;
            case 2:
            System.out.println("anda tepat sekali ");
            break;
            case 3:
            System.out.println("kurang tepat");
            break;
            case 4:
            System.out.println("sedikit lagi");
            break;
        }
    }
    
}
